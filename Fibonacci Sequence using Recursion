#include<iostream>
using namespace std;
int fibonacci(int num){
    if (num == 0 && num == 1)
    {
        return 1;
    }
    return (fibonacci(num-1)+fibonacci(num-2));
}
int main(){
    int n;
    cout << "Enter the number till you want fibonacci sequence to print" << endl;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        cout << "Fibonacci sequence generated is " << fibonacci(i) << endl;
    }
    
    return 0
}
