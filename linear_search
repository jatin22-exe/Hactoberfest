// PROGRAM FOR LINEAR SEARCH
#include<bits/stdc++.h>
int Linear_Search(int arr[],int n,int x){
    for (int i = 0; i < n; i++)
    {
        if(arr[i]==x)
        return i;

    }
    return -1;
}
int main(int argc, char const *argv[])
{
    int arr[]={56,23,78,96,01,23,63};
    int x;
    cout << "Enter the integer:";
    cin >> x;
    int n=sizeof(arr);
    cout << x << " is at index " << Linear_Search(arr,n,x);
    return 0;
}
